{
  "env": {
    "jest": true,
    "browser": true,
    "node": true,
    "es6": true,
    "webextensions": true
  },
  "extends": ["prettier"],
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": "error",
    "standard/no-callback-literal": "off"
  },
  "globals": {
    "shallow": "readonly"
  },
  "parser": "babel-eslint"
}